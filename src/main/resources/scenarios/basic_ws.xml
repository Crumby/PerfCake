<?xml version="1.0" encoding="utf-8"?>
<scenario xmlns="urn:perfcake:scenario:0.2" enabled="true">
   <quickstart name="plain_ws">
      <property name="test_property1" value="test_value1"/>
      <property name="test_property2" value="test_value2"/>
   </quickstart>
   <execution>
      <performance>
         <generator class="ImmediateMessageGenerator" threads="200">
            <property name="count" value="500000"/>
