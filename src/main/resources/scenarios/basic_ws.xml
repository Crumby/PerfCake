         <sender class="HTTPSender">
            <property name="address" value="http://${server.host}:${server.port}//jaxws-benchmark-basic/EndpointImpl"/>
         </sender>
         <messages>
            <message file="5kB_basic_reply.xml"/>
         </messages>
      </performance>
   </execution>
</scenario>
